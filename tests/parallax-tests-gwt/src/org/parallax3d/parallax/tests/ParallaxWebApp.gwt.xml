<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">

	<inherits name='org.parallax3d.parallax.platforms.parallaxGwt' />

	<inherits name='Demo' />

	<inherits name="com.google.gwt.user.theme.standard.Standard"/>

	<entry-point class='org.parallax3d.parallax.tests.client.WebApp' />
	
	<set-configuration-property name="parallax.assetpath" value="../android/assets" />

	<source path='client'/>
	<source path='resources'/>
	<source path="generator">
		<exclude name="**" />
	</source>

</module>
