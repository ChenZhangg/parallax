<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:m="urn:import:org.parallax3d.parallax.tests.client"
			 xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<ui:style>

		.sideNav {
		}

	</ui:style>

	<g:DockLayoutPanel unit='PX'>
		<!-- Main menu tree. -->
		<g:west size='250'>
			<g:DockLayoutPanel styleName="{style.sideNav}">
				<g:north size='225'>
					<m:LayoutDock ui:field='docked' />
				</g:north>
				<g:center>
					<g:VerticalPanel ui:field='menu' />
				</g:center>
			</g:DockLayoutPanel>
		</g:west>

		<!-- Main content area. -->
		<g:center>
			<g:SimpleLayoutPanel ui:field='content' />
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder> 